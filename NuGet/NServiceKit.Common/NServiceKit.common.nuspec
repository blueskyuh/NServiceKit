<?xml version="1.0"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>NServiceKit.Common</id>
    <title>Service Clients and Common libs for NServiceKit projects</title>
    <version>3.9.60</version>
    <authors>NServiceKit</authors>
    <owners>NServiceKit</owners>
    <summary>Opensource .NET and Mono REST Web Services framework</summary>
    <description>
      Common library dependency for other NServiceKit projects.
      Includes JSON, XML, JSV and SOAP Generic Service Clients.
      Contains:
      - NServiceKit.Interfaces
      - NServiceKit.Common
      Dependencies:
      - NServiceKit.Text
    </description>
    <projectUrl>https://github.com/NServiceKit/NServiceKit</projectUrl>
    <licenseUrl>https://github.com/NServiceKit/NServiceKit/blob/master/LICENSE</licenseUrl>
    <iconUrl>http://www.nservicekit.com/logo-100x100.png</iconUrl>
    <tags>NServiceKit Common Framework Clients ServiceClients Gateway</tags>
    <language>en-US</language>
    <copyright>nservicekit.com 2012 and contributors</copyright>
    <dependencies>
      <dependency id="NServiceKit.Text" version="[1,2)" />
    </dependencies>
  </metadata>
  <files>
    <file src="lib\**\*.*" target="lib" />
    <file src="..\..\src\NServiceKit.Common\**\*.cs" target="src\NServiceKit.Common" />
    <file src="..\..\src\NServiceKit.Interfaces\**\*.cs" target="src\NServiceKit.Interfaces" />
  </files>
</package>
