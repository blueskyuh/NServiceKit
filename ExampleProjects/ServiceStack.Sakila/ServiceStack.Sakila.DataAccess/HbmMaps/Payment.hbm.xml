<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ServiceStack.Sakila.DataAccess.DataModel" assembly="ServiceStack.Sakila.DataAccess">
	<class name="ServiceStack.Sakila.DataAccess.DataModel.Payment, ServiceStack.Sakila.DataAccess" table="payment" lazy="true">
		<id name="Id" column="payment_id">
			<generator class="native" />
		</id>
		<property name="Amount" column="amount" />
		<property name="PaymentDate" column="payment_date" />
		<property name="LastUpdate" column="last_update" />
		<many-to-one name="CustomerMember" column="customer_id" class="Customer" />
		<many-to-one name="RentalMember" column="rental_id" class="Rental" />
		<many-to-one name="StaffMember" column="staff_id" class="Staff" />
	</class>
</hibernate-mapping>