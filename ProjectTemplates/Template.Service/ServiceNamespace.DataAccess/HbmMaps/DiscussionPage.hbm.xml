<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="@ServiceNamespace@.DataAccess.DataModel" assembly="@ServiceNamespace@.DataAccess">
	<class name="@ServiceNamespace@.DataAccess.DataModel.DiscussionPage, @ServiceNamespace@.DataAccess" table="DiscussionPage" lazy="true">
		<id name="Id" column="Id">
			<generator class="native" />
		</id>
		<property name="CreatedDate" column="CreatedDate" />
		<property name="LastModifiedDate" column="LastModifiedDate" />
		<property name="PostCount" column="PostCount" />
		<property name="PostContent" column="PostContent" />
		<property name="PostStatistic" column="PostStatistics" />
		<many-to-one name="DiscussionMember" column="DiscussionId" class="Discussion" />
	</class>
</hibernate-mapping>