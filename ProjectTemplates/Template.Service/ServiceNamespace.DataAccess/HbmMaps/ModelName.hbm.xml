<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="@ServiceNamespace@.DataAccess.DataModel" assembly="@ServiceNamespace@.DataAccess">
	<class name="@ServiceNamespace@.DataAccess.DataModel.@ModelName@, @ServiceNamespace@.DataAccess" table="@ModelName@" lazy="true">
		<id name="Id" column="Id">
			<generator class="native" />
		</id>
		<property name="GlobalId" column="GlobalId" />
		<property name="CreatedDate" column="CreatedDate" />
		<property name="CreatedBy" column="CreatedBy" />
		<property name="LastModifiedDate" column="LastModifiedDate" />
		<property name="LastModifiedBy" column="LastModifiedBy" />
		<property name="@ModelName@Name" column="@ModelName@Name" />
		<property name="Title" column="Title" />
		<property name="Gender" column="Gender" />
		<property name="FirstName" column="FirstName" />
		<property name="LastName" column="LastName" />
		<property name="SaltPassword" column="SaltPassword" />
		<property name="Balance" column="Balance" />
		<property name="Email" column="Email" />
		<property name="Country" column="Country" />
		<property name="LanguageCode" column="LanguageCode" />
		<property name="CanNotifyEmail" column="CanNotifyEmail" />
		<property name="StoreCreditCard" column="StoreCreditCard" />
		<property name="SingleClickBuyEnabled" column="SingleClickBuyEnabled" />
		<many-to-one name="DiscussionMember" column="DiscussionId" class="Discussion" />
		<bag name="CreditCardInfos" lazy="true" cascade="all-delete-orphan"  inverse="true" >
			<key column="@ModelName@Id"></key>
			<one-to-many class="CreditCardInfo"></one-to-many>
		</bag>
		<bag name="@ModelName@Orders" lazy="true" cascade="all-delete-orphan"  inverse="true" >
			<key column="@ModelName@Id"></key>
			<one-to-many class="@ModelName@Order"></one-to-many>
		</bag>
		<bag name="@ModelName@Products" lazy="true" cascade="all-delete-orphan"  inverse="true" >
			<key column="@ModelName@Id"></key>
			<one-to-many class="@ModelName@Product"></one-to-many>
		</bag>
		<bag name="@ModelName@Sets" lazy="true" cascade="all-delete-orphan"  inverse="true" >
			<key column="@ModelName@Id"></key>
			<one-to-many class="@ModelName@Set"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>