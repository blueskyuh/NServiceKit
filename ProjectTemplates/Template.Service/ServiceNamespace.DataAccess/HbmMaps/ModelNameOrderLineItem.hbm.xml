<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="@ServiceNamespace@.DataAccess.DataModel" assembly="@ServiceNamespace@.DataAccess">
	<class name="@ServiceNamespace@.DataAccess.DataModel.@ModelName@OrderLineItem, @ServiceNamespace@.DataAccess" table="@ModelName@OrderLineItem" lazy="true">
		<id name="Id" column="Id">
			<generator class="native" />
		</id>
		<property name="Name" column="Name" />
		<property name="UnitPrice" column="UnitPrice" />
		<property name="Quantity" column="Quantity" />
		<property name="SubTotal" column="SubTotal" />
		<property name="Vat" column="Vat" />
		<property name="Total" column="Total" />
		<many-to-one name="@ModelName@OrderMember" column="@ModelName@OrderId" class="@ModelName@Order" />
	</class>
</hibernate-mapping>