<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="@ServiceNamespace@.DataAccess.DataModel" assembly="@ServiceNamespace@.DataAccess">
	<class name="@ServiceNamespace@.DataAccess.DataModel.@ModelName@Order, @ServiceNamespace@.DataAccess" table="@ModelName@Order" lazy="true">
		<id name="Id" column="Id">
			<generator class="native" />
		</id>
		<property name="@ModelName@GlobalId" column="@ModelName@GlobalId" />
		<property name="CreatedDate" column="CreatedDate" />
		<property name="CreatedBy" column="CreatedBy" />
		<property name="CardName" column="CardName" />
		<property name="CardNo" column="CardNo" />
		<property name="CardCvv" column="CardCvv" />
		<property name="CardExpiryDate" column="CardExpiryDate" />
		<property name="Total" column="Total" />
		<many-to-one name="@ModelName@Member" column="@ModelName@Id" class="@ModelName@" />
		<bag name="@ModelName@OrderLineItems" lazy="true" cascade="all-delete-orphan"  inverse="true" >
			<key column="@ModelName@OrderId"></key>
			<one-to-many class="@ModelName@OrderLineItem"></one-to-many>
		</bag>
		<bag name="@ModelName@Products" lazy="true" cascade="all"  inverse="true" >
			<key column="@ModelName@OrderId"></key>
			<one-to-many class="@ModelName@Product"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>