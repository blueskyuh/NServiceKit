<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="@ServiceNamespace@.DataAccess.DataModel" assembly="@ServiceNamespace@.DataAccess">
	<class name="@ServiceNamespace@.DataAccess.DataModel.@ModelName@Set, @ServiceNamespace@.DataAccess" table="@ModelName@Set" lazy="true">
		<id name="Id" column="Id">
			<generator class="native" />
		</id>
		<property name="CreatedDate" column="CreatedDate" />
		<property name="CreatedBy" column="CreatedBy" />
		<property name="LastModifiedDate" column="LastModifiedDate" />
		<property name="LastModifiedBy" column="LastModifiedBy" />
		<property name="Name" column="Name" />
		<property name="Type" column="Type" />
		<many-to-one name="DiscussionMember" column="DiscussionId" class="Discussion" />
		<many-to-one name="@ModelName@Member" column="@ModelName@Id" class="@ModelName@" />
		<bag name="@ModelName@SetProducts" lazy="true" cascade="all-delete-orphan"  inverse="true" >
			<key column="@ModelName@SetId"></key>
			<one-to-many class="@ModelName@SetProduct"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>