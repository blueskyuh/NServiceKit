<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="@ServiceNamespace@.DataAccess.DataModel" assembly="@ServiceNamespace@.DataAccess">
	<class name="@ServiceNamespace@.DataAccess.DataModel.Genre, @ServiceNamespace@.DataAccess" table="Genre" lazy="true">
		<id name="Id" column="Id">
			<generator class="native" />
		</id>
		<property name="Name" column="Name" />
		<many-to-one name="@ModelName@ProductMember" column="@ModelName@ProductId" class="@ModelName@Product" />
	</class>
</hibernate-mapping>