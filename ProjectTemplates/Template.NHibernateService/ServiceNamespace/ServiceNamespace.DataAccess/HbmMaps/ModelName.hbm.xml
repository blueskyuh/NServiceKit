<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="@ServiceNamespace@.DataAccess.DataModel" assembly="@ServiceNamespace@.DataAccess">
	<class name="@ServiceNamespace@.DataAccess.DataModel.@ModelName@, @ServiceNamespace@.DataAccess" table="customer" lazy="true">
		<id name="Id" column="customer_id">
			<generator class="native" />
		</id>
		<property name="FirstName" column="first_name" />
		<property name="LastName" column="last_name" />
		<property name="Email" column="email" />
		<property name="Active" column="active" />
		<property name="CreateDate" column="create_date" />
		<property name="LastUpdate" column="last_update" />
	</class>
</hibernate-mapping>